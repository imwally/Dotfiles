# Increase brightness
"xbacklight -inc 20 -steps 0"
XF86MonBrightnessUp

# Decrease brightness
"xbacklight -dec 20 -steps 0"
XF86MonBrightnessDown

# Decrease volume
"amixer set Master playback 5-"
XF86AudioLowerVolume

# Increase volume
"amixer set Master playback 5+"
XF86AudioRaiseVolume

# cmus pause 
"cmus-remote -u"
XF86AudioPlay
