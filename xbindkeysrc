# Increase brightness
"xbacklight -inc 10%"
  XF86MonBrightnessUp

# Decrease brightness
"xbacklight -dec 10%"
  XF86MonBrightnessDown

# Mute volume
"amixer set Master toggle"
  XF86AudioMute

# Decrease volume
"amixer set Master playback 5%-"
  XF86AudioLowerVolume

# Increase volume
"amixer set Master playback 5%+"
  XF86AudioRaiseVolume

# cmus pause 
"cmus-remote -u"
  XF86AudioPlay

# cmus next 
"cmus-remote -n"
  XF86AudioNext

# cmus prev 
"cmus-remote -r"
  XF86AudioPrev
